<!-- JavaScript for Search and Filtering -->
<script>
    document.addEventListener("DOMContentLoaded", function () {
        const searchBox = document.getElementById("searchBox");
        const noticeContainer = document.getElementById("noticeContainer");
        const noticeItems = document.querySelectorAll(".notice-item");
        const categoryButtons = document.querySelectorAll(".category-tabs .nav-link");

        // Search Functionality
        searchBox.addEventListener("keyup", function () {
            const input = searchBox.value.toLowerCase();
            noticeItems.forEach(notice => {
                const title = notice.querySelector(".card-title").innerText.toLowerCase();
                notice.style.display = title.includes(input) ? "block" : "none";
            });
        });

        // Category Filtering
        categoryButtons.forEach(button => {
            button.addEventListener("click", function () {
                categoryButtons.forEach(btn => btn.classList.remove("active"));
                this.classList.add("active");
                const category = this.getAttribute("data-category");
                noticeItems.forEach(notice => {
                    notice.style.display = (category === "all" || notice.getAttribute("data-category") === category) ? "block" : "none";
                });
            });
        });
    });
</script>




